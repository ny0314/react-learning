{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "skipLibCheck": true,
        "strict": true,
        "jsx": "react-jsx",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
}
